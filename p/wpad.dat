function FindProxyForURL(url, hostcs) {
    var host = hostcs.toLowerCase();
    
    if (shExpMatch(host, 'prometheus*.k*f.com')
        || shExpMatch(host, 'prometheus*.s*k.org')
        || shExpMatch(host, 'portainer*.k*f.com')
    ) { return "SOCKS localhost:8088;DIRECT"; }
        
    return "DIRECT";
}
